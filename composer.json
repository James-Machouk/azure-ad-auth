{
    "name": "james-machouk/azure-ad-auth",
    "license": "MIT",
    "description": "it's an easy and simple way to integrate azure active directory login with laravel auth",
    "require": {
        "league/oauth2-client": "dev-master"
    },
    "authors": [
        {
            "name": "James Machouk",
            "email": "dmachouk@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "JamesMachouk\\azureAdAuth\\AzureAdAuthServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "JamesMachouk\\azureAdAuth\\": "src/"
        }
    }
}
